{
  "name": "a-neural-network-wasm",
  "displayName": "üÖ∞Ô∏è Neural Network WASM",
  "description": "Beautiful neural network visualization powered by Rust WASM + MaxGraph",
  "version": "0.0.1",
  "publisher": "rustuser",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "neural",
    "network",
    "wasm",
    "rust",
    "visualization",
    "maxgraph"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "neuralnetwork.open",
        "title": "üÖ∞Ô∏è Neural Network WASM Visualizer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npx esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --skipLibCheck --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@maxgraph/core": "^0.20.0"
  }
}
